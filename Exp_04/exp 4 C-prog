#include <stdio.h>
#include <string.h>
void S(char input[]);
int belongsToLanguage(char input[]) {
  if (strlen(input) == 0) {
    return 1; 
  } else if (strlen(input) == 1) {
    if (input[0] == '0' || input[0] == '1') {
      return 1;
    } else {
      return 0; 
    }
  } else if ((input[0] == '0' && input[strlen(input)-1] == '0') || (input[0] == '1' && input[strlen(input)-1] == '1')) {
    S(input); 
    return 1;
  }
  return 0;
}
void S(char input[]) {
  if (strlen(input) == 2) {
    return;
  } else {
    int i = 1;
    while (i < strlen(input) - 1) {
      if ((input[0] == '0' && input[strlen(input)-1] == '0') && input[i] == '0') {
        printf("S -> 0S0\n");
        input[i] = 'S'; 
        S(input);
        i++; 
      } else if ((input[0] == '1' && input[strlen(input)-1] == '1') && input[i] == '1') {
        printf("S -> 1S1\n");
        input[i] = 'S'; 
        S(input);
        i++; 
      } else if (input[i] == '0' || input[i] == '1') {
        i++; 
      } else {
        return; 
      }
    }
  }
}
int main() {
  char input[100];
  printf("Enter an input string: ");
  scanf("%s", input);
  if (belongsToLanguage(input)) {
    printf("Accepted\n");
  } else {
    printf("Rejected\n");
  }
  return 0;
}
