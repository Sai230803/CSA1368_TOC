#include <stdio.h>
#include <string.h>


void S(char input[]);
void A(char input[]);


int belongsToLanguage(char input[]) {
  
  S(input); 
  return strcmp(input, "S") == 0;
}


void S(char input[]) {
  if (strlen(input) < 4 || input[0] != 'A' || input[strlen(input)-1] != 'A') {
    return; 
  } else {
    A(input); 
    int i = 1;
    while (i < strlen(input) - 1) {
      if (input[i-1] == 'A' && input[i] == '1' && input[i+1] == '0' && input[i+2] == '1' && input[i+3] == 'A') {
        printf("S -> A101A\n");
        input[i-1] = 'S'; 
        memmove(input+i, input+i+4, strlen(input)-i-3); 
        i = 1; 
        A(input); 
      } else {
        i++; 
      }
    }
  }
}

void A(char input[]) {
  if (strlen(input) == 1) {
    return; 
  } else {
    int i = 0;
    while (i < strlen(input)) {
      if (input[i] == '0' || input[i] == '1') {
        printf("A -> %cA\n", input[i]);
        i++; 
      } else {
        printf("A -> ?\n");
        input[i] = 'A'; 
        memmove(input+i+1, input+i+2, strlen(input)-i-1); 
        i++; 
      }
    }
  }
}

int main() {
  
  char input[100];
  printf("Enter an input string: ");
  scanf("%s", input);

  
  if (belongsToLanguage(input)) {
    printf("Accepted\n");
  } else {
    printf("Rejected\n");
  }

  return 0;
}
