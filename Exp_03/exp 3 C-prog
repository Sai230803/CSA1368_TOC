#include <stdio.h>
#include <string.h>
void S();
void A();
int belongsToLanguage(char input[]) {
  if (input[0] == '0' && input[strlen(input)-1] == '1') {
    int i = 1;
    while (input[i] != '1') {
      if (input[i] == '0') {
        i++;
        A(&input[i]);
      } else {
        return 0;
      }
    }
    return 1;
  }
  return 0;
}
void S() {
  printf("S -> 0A1\n");
}
void A(char input[]) {
  if (input[0] == '0') {
    printf("A -> 0A\n");
    A(&input[1]);
  } else if (input[0] == '1') {
    printf("A -> 1A\n");
    A(&input[1]);
  } else {
    printf("A -> epsilon\n");
  }
}
int main() {
  char input[100];
  printf("Enter an input string: ");
  scanf("%s", input);
  if (belongsToLanguage(input)) {
    printf("Accepted\n");
  } else {
    printf("Rejected\n");
  }
  return 0;
}
